{
  "name": "printflow-2.0-fullstack",
  "version": "2.0.0",
  "private": true,
  "description": "PrintFlow 2.0 - Complete Print Management Platform",
  "scripts": {
    "dev": "cd frontend && npm start",
    "build": "cd frontend && npm run build",
    "deploy": "netlify deploy --prod --dir=frontend/build",
    "install-all": "npm install && cd frontend && npm install && cd ../backend && npm install",
    "start": "npm run dev",
    "test": "cd frontend && npm test",
    "clean": "rm -rf frontend/build backend/node_modules frontend/node_modules node_modules"
  },
  "devDependencies": {
    "netlify-cli": "^15.8.0"
  },
  "workspaces": [
    "frontend",
    "backend"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/PrintFlow-2.0.git"
  },
  "keywords": [
    "print-management",
    "crm",
    "project-management",
    "react",
    "typescript",
    "netlify"
  ],
  "author": "PrintFlow Team",
  "license": "MIT"
}